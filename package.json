{
  "name": "element-up-theme",
  "version": "1.3.0",
  "description": "Theme generator cli tool for Element and ElementUp",
  "bin": {
    "element-up-theme": "./bin/element-theme",
    "eut": "./bin/element-theme"
  },
  "files": [
    "bin",
    "lib"
  ],
  "main": "index.js",
  "scripts": {
    "test": "xo"
  },
  "xo": {
    "space": true,
    "rule": {
      "semi": 0,
      "unicorn/no-process-exit": 0,
      "import/no-dynamic-require": 0,
      "import/no-unresolved": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:savoygu/element-up-theme.git"
  },
  "keywords": [
    "element-ui",
    "element-up",
    "vue",
    "theme",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/savoygu/element-up-theme/issues"
  },
  "author": "savoygu <savoygu@gmail.com> (https://github.com/savoygu)",
  "contributors": [
    "qingwei-li <cinwell.li@gmail.com> (https://github.com/QingWei-Li)"
  ],
  "engines": {
    "node": ">=0.12.0"
  },
  "license": "MIT",
  "peerDependencies": {
    "element-theme-chalk": "*",
    "element-up-theme-chalk": "*"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-nop": "0.0.3",
    "gulp-sass": "^3.1.0",
    "ora": "^0.3.0",
    "run-sequence": "^1.2.2",
    "sass-module-importer": "^1.4.0"
  },
  "devDependencies": {
    "element-theme-chalk": "^2.0.0-alpha.1",
    "element-ui": "^2.8.2",
    "element-up-theme-chalk": "^0.5.2",
    "xo": "^0.17.0"
  }
}
